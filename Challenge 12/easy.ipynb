      "source": [
        "def sure_hesapla(hiz, yol):\n",
        "    # Hızı yola bölerek süreyi hesapla\n",
        "    sure = yol / hiz\n",
        "    return sure\n",
        "\n",
        "hiz = 60  # km/saat\n",
        "yol = 240  # km\n",
        "\n",
        "sonuc = sure_hesapla(hiz, yol)\n",
        "print(f\"Araba {sonuc} saatte {yol} km yol alır.\")"
      ]
    }
